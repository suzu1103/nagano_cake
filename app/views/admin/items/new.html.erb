<H2>商品新規登録</H2>
<%= form_with model: @item do |f| %>
  <h4>商品画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>商品名</h4>
  <%= f.string_field :name %>
  <h4>商品説明</h4>
  <%= f.text_field :introduction %>
  <h4>ジャンル</h4>

  <h4>税抜価格</h4>
  <%= f.integer_field :price %>
  <h4>販売ステータス</h4>

  <%= f.submit '新規登録' %>
<% end %>